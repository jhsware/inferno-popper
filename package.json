{
  "name": "inferno-popper",
  "version": "5.0.1",
  "description": "Inferno wrapper around PopperJS.",
  "main": "dist/cjs/react-popper.js",
  "umd:main": "dist/umd/react-popper.js",
  "module": "lib/react-popper.js",
  "types": "inferno-popper.d.ts",
  "files": [
    "dist",
    "lib",
    "inferno-popper.d.ts"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:es && npm run build:dist && npm run build:dist:prod",
    "build:clean": "rm -rf dist/ && rm -rf lib/",
    "build:es": "babel src --out-dir lib && mv lib/index.js lib/react-popper.js",
    "build:dist": "rollup -c",
    "build:dist:prod": "cross-env NODE_ENV=production rollup -c",
    "demo": "parcel --out-dir demo/dist demo/index.html",
    "prepare": "npm run build",
    "precommit": "lint-staged",
    "format": "npm run lint -- --fix --quiet",
    "lint": "eslint --ext .js,.jsx src/ example/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jhsware/inferno-popper"
  },
  "keywords": [
    "inferno",
    "inferno-popper",
    "popperjs",
    "component",
    "drop",
    "tooltip",
    "popover"
  ],
  "lint-staged": {
    "@(src|demo)/**/*.{js,jsx}": [
      "eslint --fix --max-warnings 5",
      "git add"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jhsware/inferno-popper/issues"
  },
  "peerDependencies": {
    "inferno": "^5.0.1",
    "inferno-create-element": "^5.0.1",
    "inferno-shared": "^5.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-inferno": "^5.0.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-inferno-app": "^7.1.0",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.0.0",
    "chokidar": "^1.6.1",
    "cross-env": "^5.0.5",
    "express": "^4.15.4",
    "inferno": "^5.0.1",
    "inferno-create-element": "^5.0.1",
    "inferno-devtools": "^5.0.1",
    "inferno-router": "^5.0.1",
    "inferno-shared": "^5.0.1",
    "inferno-test-utils": "^5.0.1",
    "jest": "^20.0.4",
    "node-libs-browser": "^1.0.0",
    "outy": "^0.1.2",
    "parcel-bundler": "^1.6.2",
    "prismjs": "^1.6.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-babel-minify": "^4.0.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-globals": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0"
  }
}
