{
  "name": "inferno-popper",
  "version": "6.0.1",
  "description": "Inferno wrapper around PopperJS.",
  "main": "dist/cjs/index.js",
  "umd:main": "dist/umd/index.js",
  "module": "src/index.js",
  "types": "inferno-popper.d.ts",
  "scripts": {
    "test": "echo ok",
    "test-browser": "npm run build-test && node test/browser/server.js",
    "build-test": "rm -rf ./test/dist && cross-env NODE_ENV=development node_modules/.bin/webpack --config ./test/browser/webpack.config.js",
    "build-test-dev": "rm -rf ./test/dist && cross-env NODE_ENV=development node_modules/.bin/webpack --watch --config ./test/browser/webpack.config.js",
    "build": "npm run build:clean && npm run build:es && npm run build:dist && npm run build:dist:prod",
    "build:clean": "rm -rf dist/ && rm -rf lib/",
    "build:es": "cross-env NODE_ENV=development babel src --out-dir lib",
    "build:dist": "cross-env NODE_ENV=development rollup -c",
    "build:dist:prod": "cross-env NODE_ENV=production rollup -c",
    "demo": "parcel --out-dir demo/dist demo/index.html",
    "prepare": "npm run build",
    "precommit": "lint-staged",
    "format": "npm run lint -- --fix --quiet",
    "lint": "eslint --ext .js,.jsx src/ example/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jhsware/inferno-popper"
  },
  "keywords": [
    "inferno",
    "inferno-popper",
    "popperjs",
    "component",
    "drop",
    "tooltip",
    "popover"
  ],
  "lint-staged": {
    "@(src|demo)/**/*.{js,jsx}": [
      "eslint --fix --max-warnings 5",
      "git add"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jhsware/inferno-popper/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "classnames": "^2.2.6",
    "inferno": "^6.0.3",
    "inferno-create-element": "^6.0.3",
    "inferno-shared": "^6.0.3",
    "popper.js": "^1.14.4"
  },
  "devDependencies": {
    "bootstrap": "^4.1.3",
    "chokidar": "^1.6.1",
    "node-libs-browser": "^1.0.0",
    "outy": "^0.1.2",
    "parcel-bundler": "^1.6.2",
    "prismjs": "^1.6.0",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "7.1.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-inferno": "6.0.0",
    "babel-preset-inferno-app": "^8.0.1",
    "component-registry": "1.0.0-rc.7",
    "create-inferno-app": "^7.0.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.2",
    "inferno": "^6.0.1",
    "inferno-router": "^6.0.2",
    "inferno-create-element": "^6.0.2",
    "inferno-devtools": "^6.0.2",
    "inferno-shared": "^6.0.2",
    "inferno-test-utils": "^6.0.2",
    "isomorphic-schema": "1.0.0-beta.7",
    "jest": "^20.0.4",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-babel-minify": "^6.1.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "webpack": "^3.11.0"
  }
}
